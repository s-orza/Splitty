<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="backGround" prefHeight="240.0" prefWidth="360.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.scenes.InviteParticipantCtrl">
    <children>
        <TextField fx:id="name" layoutX="102.0" layoutY="86.0" />
        <TextField fx:id="email" layoutX="102.0" layoutY="128.0" />
      <CheckBox fx:id="add" layoutX="153.0" layoutY="176.0" mnemonicParsing="false" text="Add to evnent" />
        <Button fx:id="invite" layoutX="291.0" layoutY="86.0" mnemonicParsing="false" onAction="#invite" text="Invite" />
        <Button fx:id="cancel" layoutX="288.0" layoutY="128.0" mnemonicParsing="false" onAction="#close" text="Cancel" />
        <Label fx:id="inviteSelectParticipantsText" alignment="CENTER" layoutX="1.0" layoutY="24.0" prefHeight="30.0" prefWidth="384.0" text="Select participant to invite to the event">
            <font>
                <Font size="20.0" />
            </font>
        </Label>
        <Text fx:id="nameText" layoutX="58.0" layoutY="103.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Name" />
        <Text fx:id="emailText" layoutX="59.0" layoutY="146.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Email" />
    </children>
</AnchorPane>
